generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model menu {
  id        Int    @id @default(autoincrement())
  title     String
  path      String
  classname String
  pathroute String
  icon      String
}

model logs {
  id      Int     @id @default(autoincrement())
  method  String
  message String?
  located String
  time    String
}

model errors {
  id      Int     @id @default(autoincrement())
  errorId Int
  method  String
  message String?
  located String
  time    String
}
